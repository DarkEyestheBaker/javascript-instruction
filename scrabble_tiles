function maximumScore(tile, score) {
    let sum = 0;
    for (i = 0; i < maximumScore.length; i++) {
        sum += tile[i].score;
    }
  return sum;
}
console.log(maximumScore(1, 7));
    
maximumScore([{tile: 'N', score: 1}, 
{tile: 'K', score: 5},  
{tile: 'Z', score: 10}, 
{tile: 'X', score: 8}, 
{tile: 'D', score: 2}, 
{tile: 'A', score: 1}, 
{tile: 'E', score: 1}]);